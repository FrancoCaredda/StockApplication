<core:FragmentDefinition xmlns="sap.m" 
                         xmlns:core="sap.ui.core">
    <ColumnListItem id="itemUnavailable" type="Navigation" class="element-unavailable" press=".moveToProduct">
        <cells>
            <Label text="{products>Name} ({i18n>notAvailableStatus})" class="element-text-unavailable"/>
            <Label text="{products>Type}"/>
            <Label text="{products>Producer}"/>
            <Label text="{products>Price}"/>
            <Label text="{products>Quantity}"/>
        </cells>
    </ColumnListItem>
</core:FragmentDefinition>